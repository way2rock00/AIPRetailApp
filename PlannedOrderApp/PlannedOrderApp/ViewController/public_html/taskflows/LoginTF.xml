<?xml version="1.0" encoding="windows-1252" ?>
<adfc-mobile-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.0">
  <task-flow-definition id="LoginTF">
    <default-activity>Login</default-activity>
    <managed-bean id="__1">
      <managed-bean-name>PlannedOrderBean</managed-bean-name>
      <managed-bean-class>deloitte.retail.mobile.beans.PlannedOrderBean</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <view id="Login">
      <page>/pages/Login.amx</page>
    </view>
    <view id="SummaryPG">
      <page>/pages/SummaryPG.amx</page>
    </view>
    <control-flow-rule id="__2">
      <from-activity-id>Login</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>loginSuccess</from-outcome>
        <to-activity-id>SummaryPG</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <page-flow-scope-behavior>
      <push-new/>
    </page-flow-scope-behavior>
  </task-flow-definition>
</adfc-mobile-config>
